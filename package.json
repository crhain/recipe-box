{
  "name": "recipe-box",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://crhwebdev.github.io/fcc-recipe-box.git",
  "dependencies": {
    "lodash": "^4.17.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^6.8.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "enzyme": "^2.9.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^15.6.2",
    "selenium-standalone": "^6.14.1",
    "wdio-dot-reporter": "0.0.8",
    "wdio-mocha-framework": "^0.5.13",
    "wdio-selenium-standalone-service": "0.0.9",
    "webdriverio": "^4.12.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:sass watch:react test",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:e2e": "npm-run-all --parallel selenium-start watch:e2e-tests",
    "e2e-tests": "wdio wdio.conf.js",
    "watch:react": "react-scripts start",
    "watch:sass": "node-sass src/sass/main.scss public/css/style.css -w",
    "selenium-start": "selenium-standalone start",
    "watch:e2e-tests": "wdio wdio.conf.js --watch",
    "selenium-setup": "selenium-standalone install",
    "eject": "react-scripts eject"
  }
}
